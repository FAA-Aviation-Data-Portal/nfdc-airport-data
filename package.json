  "keywords": [
    "airport",
    "runway",
    "remarks",
    "schedules",
    "5010",
    "NASR",
    "NFDC",
    "FAA",
    "aviation"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": "ResourceDataInc/airport-data",
  "scripts": {
    "test": "standard *.js && nyc ./node_modules/.bin/mocha --opts mocha.opts test.js"
